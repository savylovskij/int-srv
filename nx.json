{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "int-srv",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "frontend-client": {
      "tags": [
        "scope: frontend-client",
        "type: app"
      ]
    },
    "frontend/client-e2e": {
      "tags": [],
      "implicitDependencies": [
        "frontend-client"
      ]
    },
    "frontend-admin": {
      "tags": [
        "scope: frontend-admin",
        "type: app"
      ]
    },
    "frontend/admin-e2e": {
      "tags": [],
      "implicitDependencies": [
        "frontend-admin"
      ]
    },
    "backend-api": {
      "tags": [
        "scope: backend-api",
        "type: app"
      ]
    }
  }
}
